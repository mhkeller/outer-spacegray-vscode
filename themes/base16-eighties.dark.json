{
    "name": "Base16 Eighties Dark",
    "tokenColors": [
        {
            "settings": {
                "background": "#2d2d2d",
                "foreground": "#d3d0c8"
            }
        },
        {
            "settings": {
                "foreground": "#d3d0c8"
            },
            "name": "Text",
            "scope": "variable.parameter.function,meta.catch meta.group variable,meta.catch punctuation.separator"
        },
        {
            "settings": {
                "foreground": "#747369"
            },
            "name": "Comments",
            "scope": "comment, punctuation.definition.comment"
        },
        {
            "settings": {
                "foreground": "#d3d0c8"
            },
            "name": "Punctuation",
            "scope": "punctuation.definition.string, punctuation.definition.variable, punctuation.definition.string, punctuation.definition.parameters, punctuation.definition.string, punctuation.definition.array"
        },
        {
            "settings": {
                "foreground": "#d3d0c8"
            },
            "name": "Delimiters",
            "scope": "none"
        },
        {
            "settings": {
                "foreground": "#cc99cc"
            },
            "name": "Operators",
            "scope": "keyword.operator"
        },
        {
            "settings": {
                "foreground": "#cc99cc"
            },
            "name": "Keywords",
            "scope": "keyword"
        },
        {
            "settings": {
                "foreground": "#f2777a"
            },
            "name": "Variables",
            "scope": "variable"
        },
        {
            "settings": {
                "foreground": "#6699cc"
            },
            "name": "Functions",
            "scope": "entity.name.function, meta.require, support.function.any-method"
        },
        {
            "settings": {
                "foreground": "#ffcc66"
            },
            "name": "Classes",
            "scope": "support.class, entity.name.type.class,support.module,support.type.object, entity.name.class, entity.name.type.class,variable.language.this"
        },
        {
            "settings": {
                "foreground": "#ffcc66"
            },
            "name": "Classes",
            "scope": "meta.entity.new.name.expr"
        },
        {
            "settings": {
                "foreground": "#6699cc"
            },
            "name": "Methods",
            "scope": "keyword.other.special-method"
        },
        {
            "settings": {
                "foreground": "#cc99cc"
            },
            "name": "Storage",
            "scope": "storage"
        },
        {
            "settings": {
                "foreground": "#66cccc"
            },
            "name": "Support",
            "scope": "support.function"
        },
        {
            "settings": {
                "foreground": "#99cc99"
            },
            "name": "Strings, Inherited Class",
            "scope": "string, constant.other.symbol, entity.other.inherited-class"
        },
        {
            "settings": {
                "foreground": "#f99157"
            },
            "name": "Integers",
            "scope": "constant.numeric"
        },
        {
            "settings": {
                "foreground": "#f99157"
            },
            "name": "Floats",
            "scope": "none"
        },
        {
            "settings": {
                "foreground": "#f99157"
            },
            "name": "Boolean",
            "scope": "none"
        },
        {
            "settings": {
                "foreground": "#f99157"
            },
            "name": "Constants",
            "scope": "constant"
        },
        {
            "settings": {
                "foreground": "#f2777a"
            },
            "name": "Tags",
            "scope": "entity.name.tag"
        },
        {
            "settings": {
                "foreground": "#f99157"
            },
            "name": "Attributes",
            "scope": "entity.other.attribute-name"
        },
        {
            "settings": {
                "foreground": "#6699cc"
            },
            "name": "Attribute IDs",
            "scope": "entity.other.attribute-name.id, punctuation.definition.entity"
        },
        {
            "settings": {
                "foreground": "#cc99cc"
            },
            "name": "Selector",
            "scope": "meta.selector"
        },
        {
            "settings": {
                "foreground": "#f99157"
            },
            "name": "Values",
            "scope": "none"
        },

        {
            "settings": {
                "foreground": "#6699cc"
            },
            "name": "Objects",
            "scope": "meta.import.js punctuation.definition.block.js, punctuation.definition.binding-pattern.object.js, punctuation.definition.binding-pattern.array.js"
        },
        {
            "settings": {
                "foreground": "#cc99cc"
            },
            "name": "Parens",
            "scope": "punctuation.separator.parameter.function,punctuation.separator.comma,punctuation.definition.group.js"
        },
        {
            "settings": {
                "foreground": "#d3d0c8"
            },
            "name": "Variables",
            "scope": "meta.brackets meta.block variable.other.readwrite.js,variable, variable.other.dollar.only.js"
        },
        {
            "settings": {
                "foreground": "#6699cc"
            },
            "name": "Variables",
            "scope": "variable.language"
        },
        {
            "settings": {
                "foreground": "#f2777a"
            },
            "name": "Variables",
            "scope": "variable.other.property.js"
        },
        {
            "settings": {
                "foreground": "#66cccc"
            },
            "name": "Special variables",
            "scope": "variable.language.self,support.constant.prototype.js"
        },
        {
            "settings": {
                "foreground": "#cc99cc"
            },
            "name": "Dollar variables",
            "scope": "punctuation.dollar.js"
        },
        {
            "settings": {
                "fontStyle": "italic"
            },
            "name": "Function arguments",
            "scope": "meta.parameters.js variable.parameter.js"
        },
        {
            "settings": {
                "fontStyle": "italic"
            },
            "name": "Export and default",
            "scope": "support.type.object.module.js, keyword.control.default.js, keyword.control.export.js"
        },
        {
            "settings": {
                "foreground": "#cc99cc"
            },
            "name": "Function parens and hard brackets",
            "scope": "meta.function.js meta.brace.round.js, meta.array.literal.js meta.brace.square.js"
        },
        {
            "settings": {
                "foreground": "#6699cc"
            },
            "name": "Curly braces",
            "scope": "punctuation.section.block.js"
        },
        {
            "settings": {
                "foreground": "#cc99cc"
            },
            "name": "Commas, key/value sep",
            "scope": "punctuation.separator.comma.js,punctuation.separator.key-value.js"
        },
        {
            "settings": {
                "fontStyle": "italic"
            },
            "name": "Object keys",
            "scope": "meta.object-literal.key.js"
        },
        {
            "settings": {
                "fontStyle": ""
            },
            "name": "Object keys",
            "scope": "meta.object-literal.key.js punctuation.separator.key-value.js"
        },

        {
            "settings": {
                "fontStyle": "",
                "foreground": "#6699cc"
            },
            "name": "Headings",
            "scope": "markup.heading punctuation.definition.heading, entity.name.section"
        },
        {
            "settings": {
                "foreground": "#f99157"
            },
            "name": "Units",
            "scope": "keyword.other.unit"
        },
        {
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffcc66"
            },
            "name": "Bold",
            "scope": "markup.bold, punctuation.definition.bold"
        },
        {
            "settings": {
                "fontStyle": "italic",
                "foreground": "#cc99cc"
            },
            "name": "Italic",
            "scope": "markup.italic, punctuation.definition.italic"
        },
        {
            "settings": {
                "foreground": "#99cc99"
            },
            "name": "Code",
            "scope": "markup.raw.inline"
        },
        {
            "settings": {
                "foreground": "#f2777a"
            },
            "name": "Link Text",
            "scope": "string.other.link"
        },
        {
            "settings": {
                "foreground": "#f99157"
            },
            "name": "Link Url",
            "scope": "meta.link"
        },
        {
            "settings": {
                "foreground": "#f99157"
            },
            "name": "Lists",
            "scope": "markup.list"
        },
        {
            "settings": {
                "foreground": "#f99157"
            },
            "name": "Quotes",
            "scope": "markup.quote"
        },
        {
            "settings": {
                "background": "#515151",
                "foreground": "#d3d0c8"
            },
            "name": "Separator",
            "scope": "meta.separator"
        },
        {
            "settings": {
                "foreground": "#99cc99"
            },
            "name": "Inserted",
            "scope": "markup.inserted, markup.inserted.git_gutter"
        },
        {
            "settings": {
                "foreground": "#f2777a"
            },
            "name": "Deleted",
            "scope": "markup.deleted, markup.deleted.git_gutter"
        },
        {
            "settings": {
                "foreground": "#cc99cc"
            },
            "name": "Changed",
            "scope": "markup.changed, markup.changed.git_gutter"
        },
        {
            "settings": {
                "foreground": "#515151"
            },
            "name": "Ignored",
            "scope": "markup.ignored, markup.ignored.git_gutter"
        },
        {
            "settings": {
                "foreground": "#515151"
            },
            "name": "Untracked",
            "scope": "markup.untracked, markup.untracked.git_gutter"
        },
        {
            "settings": {
                "foreground": "#66cccc"
            },
            "name": "Colors",
            "scope": "constant.other.color"
        },
        {
            "settings": {
                "foreground": "#66cccc"
            },
            "name": "Regular Expressions",
            "scope": "string.regexp"
        },
        {
            "settings": {
                "foreground": "#66cccc"
            },
            "name": "Escape Characters",
            "scope": "constant.character.escape"
        },
        {
            "settings": {
                "foreground": "#d27b53"
            },
            "name": "Embedded",
            "scope": "punctuation.section.embedded, variable.interpolation"
        },
        {
            "settings": {
                "background": "#f2777a",
                "foreground": "#ffffff"
            },
            "name": "Invalid",
            "scope": "invalid.illegal"
        },
        {
            "settings": {
                "foreground": "#F92672"
            },
            "name": "GitGutter deleted",
            "scope": "markup.deleted.git_gutter"
        },
        {
            "settings": {
                "foreground": "#A6E22E"
            },
            "name": "GitGutter inserted",
            "scope": "markup.inserted.git_gutter"
        },
        {
            "settings": {
                "foreground": "#967EFB"
            },
            "name": "GitGutter changed",
            "scope": "markup.changed.git_gutter"
        },
        {
            "settings": {
                "foreground": "#565656"
            },
            "name": "GitGutter ignored",
            "scope": "markup.ignored.git_gutter"
        },
        {
            "settings": {
                "foreground": "#565656"
            },
            "name": "GitGutter untracked",
            "scope": "markup.untracked.git_gutter"
        }
    ],
    "colors": {
        "sideBar.background": "#262626",
        "sideBar.foreground": "#747369",
        "statusBar.background": "#1e1e1e",
        "gitDecoration.ignoredResourceForeground": "#515151",
        "gitDecoration.modifiedResourceForeground": "#cc99cc",
        "editorGutter.modifiedBackground": "#cc99cc",
        "gitDecoration.untrackedResourceForeground": "#99cc99",
        "editorGutter.addedBackground": "#99cc99",
        "gitDecoration.deletedResourceForeground": "#f2777a",
        "editorGutter.deletedBackground": "#f2777a",
        "gitDecoration.conflictingResourceForeground": "#F92672",
        "gitDecoration.submoduleResourceForeground": "#f99157",

        "editorError.foreground": "#f2777a",

        "statusBar.noFolderBackground": "#1e1e1e",
        "statusBar.debuggingBackground": "#1e1e1e",
        "statusBar.foreground": "#515151",
        "activityBar.background": "#2d2d2d",
        "editor.background": "#2d2d2d",
        "editor.lineHighlightBorder": "#2d2d2d",
        "editor.lineHighlightBackground": "#393939",
        "editorCursor.foreground": "#d3d0c8",
        "editor.foreground": "#d3d0c8",
        "editorWhitespace.foreground": "#747369",
        "editor.selectionBackground": "#515151",
        "selection.background": "#515151",
        "list.errorForeground": "#F92672",
        "list.warningForeground": "#ffcc66",

        "list.activeSelectionBackground": "#393939",
        "list.activeSelectionForeground": "#d3d0c8",
        "list.dropBackground": "#172e45",
        "list.hoverBackground": "#262626",
        "list.hoverForeground": "#d3d0c8",

        "editorGroupHeader.tabsBorder": "#2d2d2d",
        "tab.border": "#1e1e1e",

        "editorGroup.background": "#1e1e1e",
        "tab.inactiveBackground": "#1e1e1e",
        "tab.activeBackground": "#2d2d2d",
        "tab.activeBorder": "#2d2d2d",
        "tab.hoverBackground": "#393939",
        "tab.hoverBorder": "#393939",

        "titleBar.activeBackground": "#1e1e1e",
        "titleBar.activeForeground": "#1e1e1e",
        "titleBar.inactiveBackground": "#1e1e1e",
        "titleBar.inactiveForeground": "#1e1e1e"
    },
    "colorSpaceName": "sRGB",
    "author": "Mihai Ionut Vilcu (http://github.com/ionutvmi)",
    "semanticClass": "base16.eighties.dark"
}
